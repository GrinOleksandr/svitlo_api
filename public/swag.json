                                            "additionalProperties": false
                                        },
                                        "coverArt": {
                                            "type": "object",
                                            "properties": {
                                                "url": {
                                                    "type": "string",
                                                    "nullable": true
                                                },
                                                "destinationFileName": {
                                                    "type": "string",
                                                    "nullable": true
                                                }
                                            },
                                            "additionalProperties": false
                                        },
                                        "tracks": {
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id": {
                                                        "type": "string",
                                                        "format": "uuid"
                                                    },
                                                    "resourceUrl": {
                                                        "type": "string"
